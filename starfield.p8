function _menu_draw()
	cls()
	draw_stars()
	print_msg()
end

function _menu_init()
	g.msg="the stars my destination"
	_draw=_menu_draw
	_update60=_menu_update60
	music(0)
end

function print_msg()
	print(g.msg,14,59,1)
	print(g.msg,15,60,7)
end
